      /** Popularity **/
.populer{
display: flex;
  flex-direction: row;
  flex-wrap: wrap;
}
.populer div{
	padding: 15px;
    border-top: 1px #ccc solid;
	border-right: 1px #ccc solid;
}
.populer div:nth-child(-n+4){
	border-top: none;
}
.populer div:nth-child(4n){
	border-right: none;
}
.populer article span{
	font-weight: 500;
}
.populer article small{
	font-size: 8pt;
    margin-top: 8px;
}
      .populer article img{
	float: right;
	width: 65px;
	height: 65px;
	object-fit: cover;
	margin-left: 15px;
}
.more-video{
	background-color: #000;
	color: #fff;
	padding: 10px 15px;
	border-radius: 0;
	display: table;
    margin-left: auto;
    margin-right: auto;
}
