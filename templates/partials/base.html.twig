          padding-left: 0;
          padding-right: 0;
      }
      .sosmed_wrap a{
          color: #000;
          border: 1px #000 solid;
          width: 24px;
          height: 24px;
          text-align: center;
          border-radius: 100%;
          display: table-cell;
          vertical-align: middle;
          font-size: 11px;
      }
      .topbar_two .container{
          border-bottom: 3px solid #000 !important;
          border-top: 1px solid #000 !important;
          padding: 5px;
      }
      .topbar_two .nav a {
          font-family: 'IBM Plex Serif', serif;
          font-size: 14px;
          font-weight: 300;
