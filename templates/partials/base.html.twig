      });
     
  $('.bxslider').bxSlider({
        pagerCustom: '#bx-pager',
        auto: true,
        touchEnabled: false,
        onSlideBefore: function($slideElement, oldIndex, newIndex) {
          origin_ = $('.img-headline[data-index="' + newIndex + '"]').attr("data-original");
          $('.img-headline[data-index="' + newIndex + '"]').attr('src', origin_)
        },
      });

  function adapt() {
    if(document.documentElement.clientWidth >= 768) {
      $(document).ready(function() {
        $('.bxslider_nav').bxSlider({
          controls: true,
          auto: false,
          infiniteLoop: false,
          pager: false,
          moveSlides: 5,
          touchEnabled: false,
          speed: 500,
          minSlides: 5,
          maxSlides: 15,
          slideWidth: 100,
          nextText: ' <i class="fa fa-chevron-right">&nbsp;</i>',
          prevText: ' <i class="fa fa-chevron-left">&nbsp;</i>',
          nextSelector: '#nav-next',
          prevSelector: '#nav-prev'
        });
      });
    }
  }

  window.onload = adapt();
  window.onresize = adapt();
      </script>

