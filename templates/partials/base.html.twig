              <div class="col-12 col-md-12 main_nav_cover" id="nav">							
                <span id="nav-prev"></span>
                <ul id="main-menu" class="bxslider_nav">
                  <li><a href="{{ home_url }}"><i class="fa fa-home"></i> Home</a></li>
                  {% for page in pages.children.visible %}
                    {% if page.url != home_url~'/' and page.template == 'kategori' %}
                    {% set has_child = (page.children.visible.count > 0) ? 'menu-item-has-children' : '' %}
                        <li class="{{ has_child }}">
                            <a href="{{ page.url }}">{{ page.menu }}</a>
                            {% if page.children.visible.count > 0 %}
                                {% set cek_tipe = 0 %}
                                {% for p in page.children.visible %}
                                    {% if p.template == 'kategori' %}
                                    {% set cek_tipe = cek_tipe + 1 %}
                                    {% if cek_tipe == 1 %}
                                    <ul class="sub-menu">
                                    {% endif %}
                                    <li><a style="" href="{{ p.url }}">{{ p.menu }}</a></li>   
                                    {% endif %}                                            
                                {% endfor %}
                                {% if cek_tipe > 0 %}
                                    </ul>
                                {% endif %}
                            {% endif %}        
                        </li>
                    {% endif %}
                  {% endfor %}
                </ul>
                <span id="nav-next"></span>
